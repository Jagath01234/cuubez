<web-app>
    <display-name>Archetype Created Web Application</display-name>
    <!-- 	<filter> --><!-- 		<filter-name>authFilter</filter-name> --><!-- 		<filter-class>com.cuubez.auth.filter.AuthFilter</filter-class> --><!-- 	</filter>		 -->
    <filter>
        <filter-name>keyFilter</filter-name>
        <filter-class>com.cuubez.key.KeyFilter</filter-class>
    </filter>
    <!-- 	<filter-mapping> --><!-- 		<filter-name>authFilter</filter-name> --><!-- 		<url-pattern>/*</url-pattern> --><!-- 	</filter-mapping> -->
    <filter-mapping>
        <filter-name>keyFilter</filter-name>
        <url-pattern>/account</url-pattern>
    </filter-mapping>
    <listener>
        <listener-class>com.cuubez.core.initiator.ContextLoaderListner</listener-class>
    </listener>
    <servlet>
        <servlet-name>init</servlet-name>
        <servlet-class>com.cuubez.core.initiator.ServiceInitiator</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>init</servlet-name>
        <url-pattern>/*</url-pattern>
    </servlet-mapping>
</web-app>